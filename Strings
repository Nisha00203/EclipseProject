package Strings;

public class StringCharAt {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		String str= "String functions";
		int s1= str.length();
		System.out.println(s1);
		
		char s2=str.charAt(2);
		System.out.println(s2);
	}

}


