package Strings;

public class StringSplit1 {

	public static void main(String[] args) 
	
	{
		// TODO Auto-generated method stub
		String str ="N@isha@aa";	
		String[] ch = str.split("@");
		
		for (String x : ch)
		{
			System.out.println(x);
		}
		
		 
		 

	}

}
