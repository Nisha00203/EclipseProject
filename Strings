package Strings;

public class StrinfCom {

	public static void main(String[] args)
	
	{
		String str ="India";
		String str1 = "india";
		boolean result = str.equalsIgnoreCase(str1);
		System.out.println("result is" +result);
		if(str== str1)
		{
			System.out.println("True");
			
		}
		else
			
			System.out.println("False");
	}

}

