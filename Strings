package Strings;

public class StringSub {

	public static void main(String[] args)
	
	{
		// TODO Auto-generated method stub
		String str = "saurabh";
		String str2= str.substring(3,4);
		String str1= str.substring(2, 4);
		System.out.println("substringis ="+str1);
		System.out.println("substringis ="+str2);
		
		
	

	}

}
---------------------------------------------------------------------------------------->>
package Strings;

public class SubStr {

	public static void main(String[] args) 
	{
		
		String str = "Barefoot";
		String str0= str.substring(4);
		System.out.println("substringis ="+str0);
		
		// another way
		String str1= str.substring(4,8);
		System.out.println("substringis ="+str1);

	}

}

---------------------------------------------------------------------------------------->>
package Strings;

public class Substringexample {

	public static void main(String[] args) 
	
	{
		String s1="Republicform";
		String s2=s1.substring(5);
		System.out.println(s2);
		
		String s3=s1.substring(5);
		System.out.println(s3);
		
		String s4=s1.substring(2,5);
		System.out.println(s4);
	}

}











