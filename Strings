package Strings;

import java.util.Arrays;

public class CreateObjectusingNew {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String st1 = new String("Nisha");
		String st2 = new String("Nisha");
		
		System.out.println(st1==st2);	
		System.out.println(st1.equals(st2));
		System.out.println(st1.charAt(4));
		System.out.println(st1.length());
		System.out.println(st1.indexOf('h'));
		
		
		String[] strArr =  st1.split(" ");
		System.out.println(Arrays.toString(strArr));
		
		
		//System.out.println(st1.split(" "));
		
	
	}

}


//Here st1 and st2 is pointing to 2 different object Nisha outside pool memory but inside heap
// though value is same "Nisha" it is pointing to 2 different objects hence result is  "False"

//equals method cares about value if same and not care about object referencea



---------------------------------------------------------------------------------------->>
package Strings;

public class CreateObjectwithoutNew {

	public static void main(String[] args) 
	{
		String s1 = "Nisha";
		String s2 =  "Nisha";
		String a=s1;
		
		System.out.println(s1==s2);
		
		System.out.println(s1==a);
		
	}
}



// Here s1 and s2 is pointing to same object Nisha inside Heap memo

