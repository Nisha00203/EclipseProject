package Strings;

public class FirstString {

	public static void main(String[] args) {
		
		String a ="Nisha";   // object 1
		System.out.println(a);
		a = "kush";   // creating new object kush
		System.out.println(a);
		
		// TODO Auto-generated method stub

	}

}
